{
  "name": "swagger-diff",
  "version": "0.0.0",
  "description": "Swagger Diff Utils",
  "author": "Nicolas Fortin <nicolas.fortin37@gmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint .",
    "test": "mocha --compilers js:babel/register --recursive --watch-extensions json",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test",
    "build": "npm run clean && babel src --out-dir lib && cpy src/**/*.json lib",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zallek/swagger-diff.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.0.14",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "deep-diff": "^0.3.3",
    "jsonfile": "^2.2.3",
    "lodash.defaultsdeep": "^3.10.0",
    "lodash.foreach": "^3.0.3",
    "lodash.isnumber": "^3.0.1",
    "lodash.isobject": "^3.0.2",
    "lodash.isplainobject": "^3.2.0",
    "lodash.mapvalues": "^3.0.1",
    "lodash.omit": "^3.1.0",
    "lodash.pick": "^3.1.0",
    "lodash.zipobject": "^3.0.0",
    "require-all": "^2.0.0",
    "semver": "^5.0.3",
    "swagger-parser": "^3.3.0"
  },
  "devDependencies": {
    "babel": "^5.0.8",
    "babel-core": "^5.0.8",
    "babel-eslint": "^4.1.3",
    "chai": "^3.0.0",
    "cpy": "^3.4.1",
    "eslint": "^1.4.1",
    "eslint-config-airbnb": "^0.1.0",
    "expect": "^1.6.0",
    "mocha": "^2.2.5",
    "mock-local-storage": "^1.0.2",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "yargs": "^3.5.3"
  },
  "bin": {
    "swagger": "bin/swagger-diff.js"
  }
}
